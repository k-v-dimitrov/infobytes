{
  "name": "fact-video-generator",
  "version": "1.0.0",
  "scripts": {
    "build": "rm -rf dist && tsc --project tsconfig.build.json && tsc-alias -p tsconfig.build.json && cp .env ./dist/.env",
    "dev":  "ts-node-dev -r tsconfig-paths/register ./src/index.ts",
    "dev:generator": "ts-node-dev -r tsconfig-paths/register ./src/generator.ts --id=dd0f021e-175f-49a9-8724-8f4e65e5ec1e",
    "dev:hook:pre": "ts-node-dev -r tsconfig-paths/register ./src/hooks/pre-hook.ts",
    "dev:hook:post": "ts-node-dev -r tsconfig-paths/register ./src/hooks/post-hook.ts",
    "start": "node ./dist/index.js",
    "start:generator": "node ./dist/generator.js",
    "start:frame-renderer": "node ./dist/modules/frame-renderer.js",
    "start:hook:pre": "node ./dist/hooks/pre-hook.js",
    "start:hook:post": "node ./dist/hooks/post-hook.js",
    "lint": "eslint \"./src/**/*.ts\"",
    "test": "jest"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "29.4.1",
    "prettier": "2.8.3",
    "ts-jest": "29.0.5",
    "ts-node": "10.9.1",
    "ts-node-dev": "2.0.0",
    "tsc-alias": "^1.8.2",
    "tsconfig-paths": "4.1.2",
    "typescript": "4.9.5"
  },
  "dependencies": {
    "@types/node": "^20.8.9",
    "@types/node-fetch": "^2.6.7",
    "@types/shelljs": "^0.8.14",
    "aws-sdk": "^2.1485.0",
    "canvas": "^2.11.2",
    "canvas-node": "^0.3.23",
    "dotenv": "^16.3.1",
    "execa": "5.1.1",
    "get-audio-duration": "^4.0.1",
    "knex": "^3.0.1",
    "konva": "^9.2.2",
    "node-fetch": "^2.6.6",
    "pg": "^8.11.3",
    "shelljs": "^0.8.5",
    "yargs": "^17.7.2"
  },
  "license": "UNLICENSED"
}
